function bindGrid()
{var view=$.totalStorage('display');if(!view&&(typeof displayList!='undefined')&&displayList)
    view='list';if(view&&view!='grid')
    display(view);else
    $('.display').find('li#grid').addClass('selected');$(document).on('click','#grid',function(e){e.preventDefault();display('grid');});$(document).on('click','#list',function(e){e.preventDefault();display('list');});}
function display(view)
{if(view=='list')
{$('ul.product_list').removeClass('grid').addClass('list row');$('.product_list > li').removeClass('col-xs-12 col-sm-6 col-md-4').addClass('col-xs-12');$('.product_list > li').each(function(index,element){html='';html='<div class="product-container"><div class="row">';html+='<div class="left-block col-xs-4 col-xs-5 col-md-4">'+$(element).find('.left-block').html()+'</div>';html+='<div class="center-block col-xs-4 col-xs-7 col-md-4">';html+='<div class="product-flags">'+$(element).find('.product-flags').html()+'</div>';html+='<h5 itemprop="name">'+$(element).find('h5').html()+'</h5>';var rating=$(element).find('.comments_note').html();if(rating!=null){html+='<div itemprop="aggregateRating" itemscope itemtype="http://schema.org/AggregateRating" class="comments_note">'+rating+'</div>';}
    html+='<p class="product-desc">'+$(element).find('.product-desc').html()+'</p>';var colorList=$(element).find('.color-list-container').html();if(colorList!=null){html+='<div class="color-list-container">'+colorList+'</div>';}
    var availability=$(element).find('.availability').html();if(availability!=null){html+='<span class="availability">'+availability+'</span>';}
    html+='</div>';html+='<div class="right-block col-xs-4 col-xs-12 col-md-4"><div class="right-block-content row">';var price=$(element).find('.content_price').html();if(price!=null){html+='<div class="content_price col-xs-5 col-md-12">'+price+'</div>';}
    html+='<div class="button-container col-xs-7 col-md-12">'+$(element).find('.button-container').html()+'</div>';html+='<div class="functional-buttons clearfix col-sm-12">'+$(element).find('.functional-buttons').html()+'</div>';html+='</div>';html+='</div></div>';$(element).html(html);});$('.display').find('li#list').addClass('selected');$('.display').find('li#grid').removeAttr('class');$.totalStorage('display','list');}
else
{$('ul.product_list').removeClass('list').addClass('grid row');$('.product_list > li').removeClass('col-xs-12').addClass('col-xs-12 col-sm-6 col-md-4');$('.product_list > li').each(function(index,element){html='';html+='<div class="product-container">';html+='<div class="left-block">'+$(element).find('.left-block').html()+'</div>';html+='<div class="right-block">';html+='<div class="product-flags">'+$(element).find('.product-flags').html()+'</div>';html+='<h5 itemprop="name">'+$(element).find('h5').html()+'</h5>';var rating=$(element).find('.comments_note').html();if(rating!=null){html+='<div itemprop="aggregateRating" itemscope itemtype="http://schema.org/AggregateRating" class="comments_note">'+rating+'</div>';}
    html+='<p itemprop="description" class="product-desc">'+$(element).find('.product-desc').html()+'</p>';var price=$(element).find('.content_price').html();if(price!=null){html+='<div class="content_price">'+price+'</div>';}
    html+='<div itemprop="offers" itemscope itemtype="http://schema.org/Offer" class="button-container">'+$(element).find('.button-container').html()+'</div>';var colorList=$(element).find('.color-list-container').html();if(colorList!=null){html+='<div class="color-list-container">'+colorList+'</div>';}
    var availability=$(element).find('.availability').html();if(availability!=null){html+='<span class="availability">'+availability+'</span>';}
    html+='</div>';html+='<div class="functional-buttons clearfix">'+$(element).find('.functional-buttons').html()+'</div>';html+='</div>';$(element).html(html);});$('.display').find('li#grid').addClass('selected');$('.display').find('li#list').removeAttr('class');$.totalStorage('display','grid');}}
function dropDown()
{elementClick='#header .current';elementSlide='ul.toogle_content';activeClass='active';$(elementClick).on('click',function(e){e.stopPropagation();var subUl=$(this).next(elementSlide);if(subUl.is(':hidden'))
{subUl.slideDown();$(this).addClass(activeClass);}
else
{subUl.slideUp();$(this).removeClass(activeClass);}
    $(elementClick).not(this).next(elementSlide).slideUp();$(elementClick).not(this).removeClass(activeClass);e.preventDefault();});$(elementSlide).on('click',function(e){e.stopPropagation();});$(document).on('click',function(e){e.stopPropagation();var elementHide=$(elementClick).next(elementSlide);$(elementHide).slideUp();$(elementClick).removeClass('active');});}
function accordionFooter(status)
{if(status=='enable')
{$('#footer .footer-block h4').on('click',function(){$(this).toggleClass('active').parent().find('.toggle-footer').stop().slideToggle('medium');})
    $('#footer').addClass('accordion').find('.toggle-footer').slideUp('fast');}
else
{$('.footer-block h4').removeClass('active').off().parent().find('.toggle-footer').removeAttr('style').slideDown('fast');$('#footer').removeClass('accordion');}}
function accordion(status)
{leftColumnBlocks=$('#left_column');if(status=='enable')
{var accordion_selector='#right_column .block .title_block, #left_column .block .title_block, #left_column #newsletter_block_left h4,'+'#left_column .shopping_cart > a:first-child, #right_column .shopping_cart > a:first-child';$(accordion_selector).on('click',function(e){$(this).toggleClass('active').parent().find('.block_content').stop().slideToggle('medium');});$('#right_column, #left_column').addClass('accordion').find('.block .block_content').slideUp('fast');if(typeof(ajaxCart)!=='undefined')
    ajaxCart.collapse();}
else
{$('#right_column .block .title_block, #left_column .block .title_block, #left_column #newsletter_block_left h4').removeClass('active').off().parent().find('.block_content').removeAttr('style').slideDown('fast');$('#left_column, #right_column').removeClass('accordion');}}
function calculateShpoCartHeight(hg){hg=typeof hg!=='undefined'?hg:false;var wh=parseInt($(window).outerHeight(true));if(hg!==false){if(wh<hg){$('.cart_block_list .products').css('height',490).css('overflow-y','scroll').css('overflow-x','hidden');$('.modal-box .modal-content').css('margin-top','-'+($('.modal-box .modal-content').height()/2)+'px');}}}
function changeModalBoxPosition(remove){remove=typeof remove!=='undefined'?remove:true;setTimeout(function(){var modalMargTop='-'+($('.modal-box .modal-content').outerHeight(true))/2+'px';var hh=$('.modal-box .modal-content').outerHeight(true);if(remove){$('.modal-box').removeClass('sucssMsg');$('.modal-box .modal-content').css('margin-top',modalMargTop);}else{$('.modal-box .modal-content').css('margin-top',modalMargTop);}
    calculateShpoCartHeight(hh);},10);}
function setModalCartBoxPosition(){setTimeout(function(){var $_target=$('.modal-box-cart .modal-content-cart'),modalMargTop='-'+($_target.outerHeight(true))/2+'px',hh=$_target.outerHeight(true),wh=parseInt($(window).outerHeight(true)),hg;$_target.css('margin-top',modalMargTop);hg=typeof hh!=='undefined'?hh:false;if(hg!==false){if(wh<hg){$('.cart_block_list .products').css('height',490).css('overflow-y','scroll').css('overflow-x','hidden');$_target.css('margin-top','-'+($('.modal-box .modal-content').height()/2)+'px');}}},10);}
function initResizeVideo(){setTimeout(function(){var _whh=$(window).height();resizeVideoWrap(_whh,190);},1);}
function initOpenPlResize(){setTimeout(function(){var _whh=$(window).height(),_plH=$('.videosPlaylis').height();resizeVideoWrap(_whh,_plH+130);},1);}
function resizeVideoWrap(_whhcur,_minus){var _curHe=_whhcur-_minus;$('.player-wrap').removeAttr('style').css('height',_curHe);$('.fp-ratio').removeAttr('style').css('padding-top',_curHe);$('.fp-waiting').removeAttr('style').css('margin-top',_curHe/2);}
function hideModal(){$('.modal-box').addClass('hidden').find('.modal-content').removeAttr('style').html();}
function resizeFrameVideo(){var _imgTrg=parseInt($(document).find('.img-prev > img').length),_curImgH=$(document).find('.img-prev > img').first().height(),_curImgV=$(document).find('.img-prev > img').first().width(),_frameTrg=$(document).find('.mn-video iframe');if(_imgTrg==1){_frameTrg.each(function(){$(this).height(_curImgH);$(this).width(_curImgV);});}else{_frameTrg.each(function(){var _nH=(9*$(this).width())/16
    $(this).height(_nH);});}}
function shoModalSp(cont){cont=typeof cont!=='undefined'?cont:false;$('.modal-box').find('.modal-content').html('<div class="alrt-msg"><div>'+cont+'</div></div>');if(typeof(is_first_modal_box)!=='undefined'&&is_first_modal_box==1){setTimeout(function(){$('.modal-box').removeClass('hidden');changeModalBoxPosition();},8000);}else{$('.modal-box').removeClass('hidden');changeModalBoxPosition();}}
function showModalCoupon(){var _cont=$('#modal-box-login-coupon').html();$('#modal-box-login-coupon').remove();shoModalSp(_cont);$('.modal-content').css({'width':'790px','margin-left':'-395px'});$('.modal-content .alrt-msg').css({'width':'100%','padding':'0'});$(document).find('#submitTryCoupone').on('click',function(e){e.preventDefault();tryGetCoupon();});$(document).find('#trySignUp').on('click',function(e){e.preventDefault();var $_tmpForm=$(this).parents('form');$_tmpForm.find('button').html('Sign in');$_tmpForm.find('.after-text').hide();$_tmpForm.find('.err-report').remove();$_tmpForm.find('#is_new_customer_c').val('1');$_tmpForm.find('.form-group').each(function(){if(!$(this).hasClass('is-customer')){$(this).remove();}else{$(this).removeClass('hidden');}});});}
function validate_isName(s){var reg=/^[^0-9!<>,;?=+()@#"ТА{}_$%:]+$/;return reg.test(s);}
function validate_isEmail(s){var reg=unicode_hack(/^[a-z\p{L}0-9!#$%&'*+\/=?^`{}|~_-]+[.a-z\p{L}0-9!#$%&'*+\/=?^`{}|~_-]*@[a-z\p{L}0-9]+[._a-z\p{L}0-9-]*\.[a-z\p{L}0-9]+$/i,false);return reg.test(s);}
function tryGetCoupon(){var $_form=$('.modal-content #modal-box-login-coupon_form'),customerEmail=$_form.find('#email_c').val(),_errCount=0,_curValue='',fn_name='';$_form.find('.err-report').remove();$_form.find('.form-error').removeClass('form-error');$_form.find('input').each(function(){if($(this).hasClass('is_required')){_curValue=$(this).val();fn_name=new Function('validate_'+$(this).data('validate'));if(_curValue==''||fn_name(_curValue)){$(this).parent().addClass('form-error');++_errCount;}}});if(_errCount!=0){return false;}else{$.ajax({url:baseUri+'?rand='+new Date().getTime(),async:true,cache:false,type:'POST',dataType:"json",headers:{"cache-control":"no-cache"},data:{controller:'authentication',action:'tryGetCoupon',token:token,ajax:1,email:customerEmail,ac:$_form.find('#is_new_customer_c').val(),uName:$_form.find('#customer_firstname_c').val(),uSName:$_form.find('#customer_lastname_c').val(),passwd:$_form.find('#passwd_c').val(),uGroupe:$_form.find('#modalbox_join_groupe').val(),uCartRule:$_form.find('#modalbox_join_coupone').val(),uSubject:$_form.find('#modalbox_join_esubject').val()},success:function(data){if(data.hasError){$_form.find('h3').after('<div class="err-report">'+data.errors+'</div>');}else{ga('send','event','popupWindow','submitSuccess','Get promo code');$_form.after('<div class="scs-report" style="position: absolute;top: 50%;left: 440px;right: 40px;">'+data.msg+'</div>');$_form.remove();}}});}}
