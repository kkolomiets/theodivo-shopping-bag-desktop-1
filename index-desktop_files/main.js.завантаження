        twitter_btn=this.ref.find('.twitter a');if(twitter_btn.length===1){var p_manufacturer=twitter_btn.attr('data-manufacturer'),p_title=twitter_btn.attr('data-title');if(typeof(p_manufacturer)!=='undefined'&&typeof(p_title)!=='undefined'){twitter_url=encodeURI("https://twitter.com/intent/tweet?text=Look at @The_Odivo "+p_manufacturer+' '+this.loc+' - '+p_title);}else{twitter_url=encodeURI("https://twitter.com/intent/tweet?text=Look at @The_Odivo "+this.meta_title+"&url="+this.loc);}
            this.setButton(twitter_btn,twitter_url);}
        vkontakte_btn=this.ref.find('.vk a');if(vkontakte_btn.length===1){vkontakte_url=encodeURI("https://vk.com/share.php?url="+this.loc+"&image="+this.meta_image);this.setButton(vkontakte_btn,vkontakte_url);}
